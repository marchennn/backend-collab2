generator db{
    provider = "prisma-client-py"
    recursive_type_depth = 5
}

datasource database{
    provider = "postgresql"
    url = env("DB_URL")
}

model sensor{
    id Int @id @default(autoincrement())
    temperature Float
    humidity Float
    distance1 Float
    distance2 Float
    timestamp DateTime @default(now())
    update_at DateTime @default(now()) @updatedAt

    @@map("sensor")
}

model led{
    id Int @id @default(autoincrement())
    led1 String
    led2 String
    timestamp DateTime @default(now())
    update_at DateTime @default(now()) @updatedAt

    @@map("led")
}

model user{
    id Int @id @default(autoincrement())
    device String
    create_at DateTime @default(now())
    update_at DateTime @default(now())

    @@map("user")
}